{
  "ReverseProxy": {
    "Routes": {
      "webhost": {
        "ClusterId": "webhost",
        "Match": { "Path": "/api/{**catch-all}" },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      }
    },
    "Clusters": {
      "webhost": {
        "Destinations": {
          "d1": { "Address": "http://localhost:5054" }
        }
      }
    }
  }
}
